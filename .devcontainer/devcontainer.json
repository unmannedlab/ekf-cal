{
    "name": "ROS2-env",
    "customizations": {
        "vscode": {
            "extensions": [
                "fabiospampinato.vscode-todo-plus",
                "mhutchie.git-graph",
                "ms-iot.vscode-ros",
                "ms-python.python",
                "ms-vscode.cpptools-extension-pack",
                "ms-vscode.cpptools",
                "streetsidesoftware.code-spell-checker",
                "twxs.cmake",
                "zachflower.uncrustify",
                "ms-python.flake8"
            ]
        }
    },
    "build": {
        "dockerfile": "Dockerfile"
    },
    "workspaceMount": "src=${localWorkspaceFolder},dst=/ekf_cal_ws/src/ekf_cal,type=bind,consistency=cached",
    "workspaceFolder": "/ekf_cal_ws/src/ekf_cal"
}