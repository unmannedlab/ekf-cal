{
    "version": "2.0.0",
    "configurations": [
        {
            "name": "ekf_cal",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/../../install/ekf_cal/lib/ekf_cal/ekf_cal_node",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        },
        {
            "name": "ROS: Attach",
            "type": "node",
            "request": "attach"
        },
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Test Plot",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/eval/plot.py",
            "args": [
                "config/fiducial.yaml"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Test Bokeh Plot",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/eval/plot-bokeh.py",
            "args": [
                "config/fiducial.yaml"
            ],
            "console": "integratedTerminal"
        }
    ],
}